+++ b/src/main/java/org/bukkit/Server.java
@@ -1464,4 +1464,15 @@ public interface Server extends PluginMessageRecipient {
      */
     int getCurrentTick();
     // Paper end
+    
+    // YAPFA start
+    
+    /**
+     * Get the ms the last servertick took
+     *
+     * @return Last ticks ms
+     */
+    long getLastTickMs();
+    
+    // YAPFA end
 }
